{
    "name": "seyfer/kohana-mpdf",
    "version": "3.2",
    "type": "kohana-module",
    "description": "Kohana module to manage pdf quick and easy",
    "type": "kohana-module",
    "authors": [
        {
            "name": "seyfer",
            "email": "seyferseed@mail.ru",
            "homepage": "http://seyferseed.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
